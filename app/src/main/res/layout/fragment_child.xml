<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context=".ChildFragment">





        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            >


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >




          <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:gravity="center"
              >


              <ImageView
                  android:id="@+id/child1"
                  android:layout_width="160dp"
                  android:layout_height="240dp"
                  android:background="@drawable/img_19"
                  android:layout_row="0"
                  android:layout_margin="8dp"
                  android:layout_column="0"

                  />


              <ImageView
                  android:id="@+id/child2"
                  android:layout_width="160dp"
                  android:layout_height="240dp"
                  android:background="@drawable/img_29"
                  android:layout_row="0"
                  android:layout_margin="8dp"
                  android:layout_column="1"

                  />




          </LinearLayout>







            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                >





            <ImageView
            android:id="@+id/child3"
            android:layout_width="160dp"
            android:layout_height="240dp"
            android:background="@drawable/img_30"
            android:layout_row="1"
            android:layout_margin="8dp"
            android:layout_column="0"

            />


        <ImageView
            android:id="@+id/child4"
            android:layout_width="160dp"
            android:layout_height="240dp"
            android:background="@drawable/img_31"
            android:layout_row="1"
            android:layout_margin="8dp"
            android:layout_column="1"

            />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                >



            <ImageView
            android:id="@+id/child5"
            android:layout_width="160dp"
            android:layout_height="240dp"
            android:background="@drawable/img_32"
            android:layout_row="2"
            android:layout_margin="8dp"
            android:layout_column="0"

            />


        <ImageView
            android:id="@+id/child6"
            android:layout_width="160dp"
            android:layout_height="240dp"
            android:background="@drawable/img_33"
            android:layout_row="2"
            android:layout_margin="8dp"
            android:layout_column="1"

            />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                >

        <ImageView
            android:id="@+id/child7"
            android:layout_width="160dp"
            android:layout_height="240dp"
            android:background="@drawable/img_34"
            android:layout_row="3"
            android:layout_margin="8dp"
            android:layout_column="0"

            />


        <ImageView
            android:id="@+id/child8"
            android:layout_width="160dp"
            android:layout_height="240dp"
            android:background="@drawable/img_35"
            android:layout_row="3"
            android:layout_margin="8dp"
            android:layout_column="1"

            />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                >

            <ImageView
            android:id="@+id/child9"
            android:layout_width="160dp"
            android:layout_height="240dp"
            android:background="@drawable/img_36"
            android:layout_row="4"
            android:layout_margin="8dp"
            android:layout_column="0"

            />


        <ImageView
            android:id="@+id/child10"
            android:layout_width="160dp"
            android:layout_height="240dp"
            android:background="@drawable/img_37"
            android:layout_row="4"
            android:layout_margin="8dp"
            android:layout_column="1"

            />

            </LinearLayout>



            </LinearLayout>


        </ScrollView>









</LinearLayout>